 @model IEnumerable<CMD.Entities.GetParticularPatient_Result>

@{ ViewBag.Title = "IndexGP"; }

<style>
    body {
        padding: 0;
        margin: 0 0 10px 0;
    }

    .row {
        margin: 0 !important
    }

    #profileImage {
        cursor: pointer;
        width: 90px;
        height: 90px;
        overflow: hidden;
        -webkit-border-radius: 50%;
        -moz-border-radius: 50%;
        -ms-border-radius: 50%;
        -o-border-radius: 50%;
        border-radius: 50%;
    }

    #profileImgD {
        cursor: pointer;
        width: 50px;
        height: 50px;
        overflow: hidden;
        -webkit-border-radius: 50%;
        -moz-border-radius: 50%;
        -ms-border-radius: 50%;
        -o-border-radius: 50%;
        border-radius: 50%;
    }

    .profile_data {
        margin: 0px;
    }

    #profile_pic {
        width: 40px;
        height: 40px;
        border-radius: 50%;
    }


    .cards {
        box-shadow: rgba(0, 0, 0, 0.35) 0px 5px 15px;
        width: 990px;
        margin-left: 40px;
        margin-bottom: 20px;
        padding-bottom: 20px;
        padding-top:20px;
    }

    .vl {
        border-right: 1px solid black;
        height: 100px;
    }

    .vls {
        border-right: 1px solid black;
        height: 20px;
    }

    ::-webkit-scrollbar {
        width: 5px;
        height: 4px;
        border: 1px solid black;
    }

    .card_body {
        box-shadow: rgba(0, 0, 0, 0.35) 0px 5px 15px;
        height: 150px;
    }

    .card_body1 {
        box-shadow: rgba(0, 0, 0, 0.35) 0px 5px 15px;
        width: 350px;
        height: 200px;
    }

    .card_body2 {
        box-shadow: rgba(0, 0, 0, 0.35) 0px 5px 15px;
        width: 350px;
        height: 40px;
    }

    .heading {
        color: blue;
        font-size: 20px;
        position: fixed;
    }

    .Presc {
        max-height: 300px;
        overflow-y: auto;
    }

    .profile-icons {
        color: #1e90ff;
        padding: 5px;
        font-size: 20px;
    }

    .profile-data {
        margin-left: 10px;
    }

    .company-name {
        font-size: 12px;
        font-weight: bold;
        padding-left: 5px;
    }

    .col-lg-2 {
        padding: 0 4px !important;
    }

    li {
        padding: 0.2rem 1rem !important;
        font-size: 12px !important;
    }

    .main-part {
        background-color: white;
        padding-left: 1.5rem;
        width: 90vw !important;
        padding-bottom: 1.5rem;
    }

    .body-content {
        padding: 1px 0 !important;
    }

    .col-lg-10 {
        padding-left: 0 !important;
    }
</style>



<div class="main-part " style="padding:0;margin:0;">
    <div class="row" style="padding:10px; margin-left:0 10px;margin-top:12px; box-shadow:5px 5px 10px grey; height:170px;">

        <div class="col-1 image">
            <image id="profileImage" src="http://lorempixel.com/100/100" />
        </div>
        @foreach (var item in Model)
        {
<div class="col-2 profile-data">
    <h4>@item.patientName</h4>
    <h7>Tel:876-833-2323423</h7><br>
    <i class="fas fa-comment-dots profile-icons "></i>
    <i class="fas fa-phone-alt profile-icons "></i>
    <i class="fas fa-video profile-icons "></i>

</div>

                <div class="col-2 vl pl-4">
                    <h5 style="color:gray">Info</h5>
                    <h6>@item.DOB</h6>
                    <h6>Age: @item.Age</h6>
                    <h6>Height: @item.Height cm</h6>
                    <h6><i class="fas fa-tint" style="color:red; font-size:14px; padding-right:8px;"></i> @item.BloodGroup</h6>
                </div>
                                <div class="col-1 vl">
                                    <h5 style="color:gray">Allergies</h5>
                                    <h6>@item.Allergies</h6>
                                </div>
                                                <div class="col-2 vl">
                                                    <h5 style="color:gray; text-align:center;">Active Issues</h5><br>
                                                    <center>-</center>
                                                </div>
                                                                <div class="col-2">
                                                                    <h5 style="color:gray">Medical Problems</h5>
                                                                    <h6>@item.MedicalProblem</h6>
                                                                </div>break;
                                                                            }
    </div>
    <h4 class="mt-5" style="color:dodgerblue; margin-left:20px;"><b>Appointments/Consultations</b></h4><br>
    <div class="cards bg-light">
        <div class="row" style="margin:10px;">

            <div class="col-lg-4 " style="border:3px">
                <h4 style="color:dodgerblue">Symptom History</h4>
                <hr>
                <div class="Presc">

                    @foreach (var item in Model)
                    {
        <div class="card_body1 p-3 mx-4 " style="font-size:small">
            <div class="row">
                <div class="col-md-12">
                    <h4>@item.SymptomName</h4>
                    <h8>Date & Time<br>@item.DateTime_Appoinment</h8>
                    <hr>
                    <h5><b>@item.Reading</b></h5>
                    <span class="prescription">
                        Doctor :@item.DoctorName



                    </span>
                </div>
            </div>



        </div><br>}
                </div>
            </div>
            <div class="col-lg-4 ">
                <h4 style="color:dodgerblue">Appointment History</h4>
                <hr>
                <div class="Presc">
                    @foreach (var item in Model)
                    {
        <div class="card_body1 p-3 mx-4" style="font-size:small">
            <div class="row">
                <div class="col-1 image">
                    <image id="profileImgD" src="data:image/png;base64,"> </image>
                </div>


                <span class="p-3" style="font-size:14px; font-weight:bold;"><i class="fas fa-stethoscope"></i> @item.DoctorName</span>
                <h8>Date & Time<br>@item.DateTime_Appoinment</h8>
                <hr><br />
                <h6 style="font-size:16px;"><b>@item.DoctorsNote</b></h6>


            </div>



        </div><br>}
                </div>
            </div>
            <div class="col-lg-4">
                <h4 style="color:dodgerblue">Prescription History</h4>
                <hr>
                <div class="Presc">
                    @foreach (var item in Model)
                    {
        <div class="card_body1 p-3 mx-4" style="font-size:small">
            <div class="row">
                <div class="col-md-12">
                    <h4>@item.SymptomName</h4>
                    <h8>Date & Time<br>@item.DateTime_Appoinment</h8>
                    <hr>
                    <h6 style="font-size:16px;"><b>@item.Medicine_Name</b></h6>
                    <span class="prescription">
                        <p>
                            usage : @item.Medicine_Cycle
                            Instructions :@item.Comment
                        </p>

                    </span>
                </div>
            </div>



        </div><br>}
                </div>
            </div>
        </div>


    </div>

</div>